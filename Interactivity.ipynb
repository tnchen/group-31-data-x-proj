    "                        tiles=\"Mapbox Bright\")\n",
    "folium.GeoJson('us_counties_20m.json', name='US Counties').add_to(folium_map)\n",
    "folium.GeoJson('us_states_20m.json', name='US States').add_to(folium_map)\n",
    "folium.LayerControl().add_to(folium_map)\n",
    "embed_map(folium_map)"
